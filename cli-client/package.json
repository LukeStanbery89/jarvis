{
    "name": "@jarvis/cli-client",
    "version": "1.0.0",
    "description": "CLI client for J.A.R.V.I.S. audio playback",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "audio-ws-client": "./dist/bin/audio-ws-client.js",
        "audio-ws-server": "./dist/bin/audio-ws-server.js",
        "play-audio": "./dist/bin/play-audio.js",
        "stream-audio": "./dist/bin/stream-audio.js",
        "test-codec": "./dist/bin/test-codec.js",
        "wav-to-pcm": "./dist/bin/wav-to-pcm.js"
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "clean": "rm -rf dist",
        "rebuild": "npm run clean && npm run build"
    },
    "keywords": [
        "audio",
        "cli",
        "jarvis"
    ],
    "author": "Luke Stanbery",
    "license": "ISC",
    "dependencies": {
        "@jarvis/audio": "^1.0.0",
        "@jarvis/audio-stream": "^1.0.0",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.10",
        "typescript": "^5.3.3"
    }
}
