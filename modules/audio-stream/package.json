{
    "name": "@jarvis/audio-stream",
    "version": "1.0.0",
    "description": "High-level API for audio WebSocket streaming with reusable core classes and standalone wrappers",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./core": {
            "types": "./dist/core/index.d.ts",
            "import": "./dist/core/index.js"
        },
        "./standalone": {
            "types": "./dist/standalone/index.d.ts",
            "import": "./dist/standalone/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "clean": "rm -rf dist",
        "rebuild": "npm run clean && npm run build",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "keywords": [
        "audio",
        "streaming",
        "websocket",
        "real-time",
        "jarvis"
    ],
    "author": "Luke Stanbery",
    "license": "ISC",
    "dependencies": {
        "@jarvis/audio": "^1.0.0",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.10",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    }
}