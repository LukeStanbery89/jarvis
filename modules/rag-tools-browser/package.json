{
    "name": "@jarvis/rag-tools-browser",
    "version": "1.0.0",
    "description": "Browser extension tools for RAG operations (page extraction, navigation)",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "clean": "rm -rf dist",
        "rebuild": "npm run clean && npm run build",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@jarvis/protocol": "^1.0.0",
        "@jarvis/ws-client": "^1.0.0"
    },
    "devDependencies": {
        "@types/chrome": "^0.0.268",
        "@vitest/ui": "^1.1.3",
        "jsdom": "^24.0.0",
        "typescript": "^5.3.3",
        "vitest": "^1.1.3"
    },
    "author": "Luke Stanbery",
    "license": "ISC"
}
